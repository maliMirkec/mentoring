{
  "name": "ui-mentoring",
  "version": "1.0.0",
  "description": "Become a better UI developer today.",
  "main": "index.js",
  "repository": "git@github.com:maliMirkec/mentoring-tips.git",
  "author": "Star Bist <malimirkeccita@gmail.com>",
  "license": "MIT",
  "scripts": {
    "cp_redirects": "cp extras/_redirects public/_redirects",
    "cp_headers": "cp extras/_headers public/_headers",
    "cp_robots-dev": "cp extras/robots_dev.txt public/robots.txt",
    "cp_robots-prod": "cp extras/robots_prod.txt public/robots.txt",
    "eleventy-dev": "env ELEVENTY_ENV=development npx @11ty/eleventy@0.12.1 --serve --watch",
    "eleventy-build": "env ELEVENTY_ENV=production npx @11ty/eleventy@0.12.1",
    "dev": "npm run eleventy-dev",
    "gulp": "concurrently --kill-others \"npm run eleventy-dev\" \"gulp\"",
    "preview": "gulp build && npm run eleventy-build && npm run cp_redirects && npm run cp_headers && npm run cp_robots-dev",
    "build": "gulp build && npm run eleventy-build && npm run cp_redirects && npm run cp_headers && npm run cp_robots-prod"
  },
  "dependencies": {
    "browser-sync": "^2.27.10",
    "concurrently": "^7.3.0",
    "del": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-bump": "^3.2.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-cssimport": "^7.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-exit": "^0.0.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-include": "^2.4.1",
    "gulp-penthouse": "^0.2.0",
    "gulp-real-favicon": "^0.3.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-wait": "^0.0.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "modularscale-sass": "^3.0.10",
    "normalize.css": "^8.0.1",
    "sass-mq": "^6.0.0",
    "stylelint": "^14.11.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "uslug": "^1.0.4"
  }
}
